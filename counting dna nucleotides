dataset = 'AGCTTTTCATTCTGACTGCAACGGGCAATATGTCTCTGTGTGGATTAAAAAAAGAGTGTCTGATAGCAGC'

A = dataset.count('A')
C = dataset.count('C')
G = dataset.count('G')
T = dataset.count('T')

print("A:", A, "C:", C, "G:", G, "T:", T)
            
